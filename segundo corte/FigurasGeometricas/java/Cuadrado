class Cuadrado {
    private double lado;

    public Cuadrado(double lado) {
        this.lado = lado;
    }

    public double calcularArea() {
        return lado * lado;
    }
    public double calcularPerimetro(){
        return lado * 4;
    }

    public static void main(String[] args) {
        Cuadrado miCuadrado = new Cuadrado(5);
        System.out.println("El Ã¡rea del cuadrado es: " + miCuadrado.calcularArea());
        System.out.println("El perimetro del cuadrado es: " + miCuadrado.calcularPerimetro());
    }
}
